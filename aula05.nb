Cell[TextData[{
 "As equa\[CCedilla]\[OTilde]es de movimento s\[ATilde]o as seguintes:\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         FractionBox[
          RowBox[{
            SuperscriptBox[
             StyleBox["d", "TI"], "2"], 
            SubscriptBox[
             StyleBox["x", "TI"], "1"]}], 
          RowBox[{
            StyleBox["d", "TI"], 
            SuperscriptBox[
             StyleBox["t", "TI"], "2"]}]], "\[LongEqual]", "-", 
         FractionBox[
          StyleBox["k", "TI"], 
          StyleBox["m", "TI"]], 
         SubscriptBox[
          StyleBox["x", "TI"], "1"], "+", 
         FractionBox[
          StyleBox["k", "TI"], 
          StyleBox["m", "TI"]], 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["x", "TI"], "2"], "-", 
             SubscriptBox[
              StyleBox["x", "TI"], "1"]}], ")"}], ","}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\frac{d^2 \
x_1}{dt^2}=-\\frac{k}{m}x_1+\\frac{k}{m}\\left(x_2-x_1\\right),", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "56b59076-1fdf-443d-9704-d58059621b71"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         FractionBox[
          RowBox[{
            SuperscriptBox[
             StyleBox["d", "TI"], "2"], 
            SubscriptBox[
             StyleBox["x", "TI"], "2"]}], 
          RowBox[{
            StyleBox["d", "TI"], 
            SuperscriptBox[
             StyleBox["t", "TI"], "2"]}]], "\[LongEqual]", "-", 
         FractionBox[
          StyleBox["k", "TI"], 
          StyleBox["m", "TI"]], 
         SubscriptBox[
          StyleBox["x", "TI"], "2"], "-", 
         FractionBox[
          StyleBox["k", "TI"], 
          StyleBox["m", "TI"]], 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["x", "TI"], "2"], "-", 
             SubscriptBox[
              StyleBox["x", "TI"], "1"]}], ")"}], "."}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\frac{d^2 \
x_2}{dt^2}=-\\frac{k}{m}x_2-\\frac{k}{m}\\left(x_2-x_1\\right).", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "fb96c660-7e39-492b-a8fd-b5243e1ef4ff"]
}], "Text",
